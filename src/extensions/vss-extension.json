{
  "manifestVersion": 1,
  "id": "build-release-task",
  "name": "Capgemini Microsoft UK Build and Release Tools",
  "version": "1.0.2",
  "publisher": "capgemini-msft-uk",
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "icons": {
    "default": "logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tools for helping the release and testing process.",
  "categories": [
    "Azure Pipelines"
  ],
  "files": [
    {
      "path": "tasks/release-notes"
    },
    {
			"path": "tasks/owasp-scan"
		},
		{
			"path": "images",
			"addressable": true     
		}
  ],
  "contributions": [
    {
      "id": "releasenotes",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/release-notes"
      }
    },
    {
			"id": "owaspapiscan",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "tasks/owasp-scan"
			}
		}
  ]
}