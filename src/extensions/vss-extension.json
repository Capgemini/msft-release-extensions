		"targets": [
		  "ms.vss-distributed-task.tasks"
		],
		"properties": {
		  "name": "tasks/azuresearch/ConfigureAzureSearch"
		}
	  },
	  {
		"id": "azuresearchaccesslevel",
		"type": "ms.vss-distributed-task.task",
		"targets": [
		  "ms.vss-distributed-task.tasks"
		],
		"properties": {
		  "name": "tasks/azuresearch/ConfigureAzureSearchAccessLevel"
		}
	  },
	  {
		"id": "synapsepipelinetriggers",
		"type": "ms.vss-distributed-task.task",
