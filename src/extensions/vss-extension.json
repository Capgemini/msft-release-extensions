	  }
	],
	"contributions": [
	  {
		"id": "releasenotes",
		"type": "ms.vss-distributed-task.task",
		"targets": [
		  "ms.vss-distributed-task.tasks"
		],
		"properties": {
		  "name": "tasks/release-notes"
		}
	  },
	  {
		"id": "owaspapiscan",
		"type": "ms.vss-distributed-task.task",
		"targets": [
		  "ms.vss-distributed-task.tasks"
		],
		"properties": {
		  "name": "tasks/owasp-scan"
		}
	  },
	  {
